name: DPSDGYM Evaluation

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      dataset: {type: string,  default: "bank"}
      eval_metrics: {type: string, default: "pmse ml_eval wasserstein"}
      epsilons: {type: string, default: "1.0"}
    command: "python main.py -d {dataset} -m {eval_metrics} -e {epsilons}"